{
  "manifest_version": 3,
  "name": "QBrowser",
  "version": "1.0",
  "description": "A browser extension for quick navigation",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}